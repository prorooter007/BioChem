<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context=".Activity_4">


  <Button
      android:id="@+id/button6_2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="78dp"
      android:text="Back"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/button8_2" />

  <Button
      android:id="@+id/button7_2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="13dp"
      android:layout_marginEnd="28dp"
      android:text="Test"
      app:layout_constraintBaseline_toBaselineOf="@+id/button8_2"
      app:layout_constraintEnd_toStartOf="@+id/button8_2"
      app:layout_constraintHorizontal_chainStyle="packed"
      app:layout_constraintStart_toStartOf="parent" />


  <Button
      android:id="@+id/button8_2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="75dp"
      android:text="Result"
      app:layout_constraintBottom_toTopOf="@+id/button6_2"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/button7_2"
      app:layout_constraintTop_toBottomOf="@+id/imageView6_2" />

  <ImageView
      android:id="@+id/imageView3_2"
      android:layout_width="443dp"
      android:layout_height="755dp"
      android:layout_marginStart="4dp"
      android:layout_marginTop="4dp"
      android:scaleType="fitXY"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/lab" />

  <TextView
      android:id="@+id/textView4_2"
      android:layout_width="301dp"
      android:layout_height="49dp"
      android:layout_marginStart="49dp"
      android:layout_marginTop="37dp"
      android:layout_marginEnd="49dp"
      android:layout_marginBottom="37dp"
      android:textAlignment="center"
      android:text=""
      android:textColor="@color/black"
      android:textSize="32dp"
      android:textStyle="bold"
      app:layout_constraintBottom_toTopOf="@+id/imageView6_2"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <androidx.camera.view.PreviewView
      android:id="@+id/imageView6_2"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_marginStart="52dp"
      android:layout_marginEnd="52dp"
      android:layout_marginBottom="21dp"
      android:scaleType="fitCenter"
      android:visibility="visible"
      app:layout_constraintBottom_toTopOf="@+id/button8_2"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView4_2"
      tools:ignore="MissingClass">

  </androidx.camera.view.PreviewView>

  <ImageView
      android:id="@+id/imageView_box_2"
      android:layout_width="85dp"
      android:layout_height="82dp"
      android:src="@drawable/cam_box"
      app:layout_constraintBottom_toBottomOf="@+id/imageView6_2"
      app:layout_constraintEnd_toEndOf="@+id/imageView6_2"
      app:layout_constraintStart_toStartOf="@+id/imageView6_2"
      app:layout_constraintTop_toTopOf="@+id/imageView6_2" />


</androidx.constraintlayout.widget.ConstraintLayout>